
# A dynamic programming evaluator generated by GAP-C.
# 
#   GAP-C version:
#     gapc
# 
#   GAP-C call:
#     gapc -p affine * pretty --backtrace --cyk affinelocsim.gap 
# 
# 


RTLIB_LDFLAGS = $(RT_LDFLAGS)
RTLIB_LDLIBS = $(RT_LDLIBS)
RTLIB_CPPFLAGS = $(RT_CPPFLAGS)

ifeq "$(origin NO_CONFIG_MF)" "undefined"
$(info Including global makefile /homes/fkeller/share/gapc/config_linux-gnu.mf)
-include /homes/fkeller/share/gapc/config_linux-gnu.mf
endif

-include gapc_local.mf

ifdef MF
$(info Including extra makefile $(MF))
include $(MF)
endif

CXXFILES =  out_main.cc out.cc

DEPS = $(CXXFILES:.cc=.d)
OFILES = $(CXXFILES:.cc=.o) string.o

out : $(OFILES)
	$(CXX) -o $@ $^  $(LDFLAGS) $(LDLIBS) $(CXXFLAGS_OPENMP) 

out_main.cc : $(RTLIB)/generic_main.cc out.cc
	echo '#include "out.hh"' > $@
	cat $(RTLIB)/generic_main.cc >> out_main.cc

basenameCXX=$(shell basename $(CXX))
ifneq ($(filter $(basenameCXX),g++ icc),)
-include $(DEPS)

%.o : %.cc
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) $< -c -o $@ 
endif

.PHONY: clean
clean:
	rm -f $(OFILES) out out_main.cc

string.o: $(RTLIB)/string.cc
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c $< -o $@
